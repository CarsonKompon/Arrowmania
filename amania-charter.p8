function _update60()
	msx=stat(32)
	msy=stat(33)
	mkey=stat(31)
	mbt=0b001&stat(34)==0b001
	mrt=0b010&stat(34)==0b010
	olp=""
	if msx>50 and msx<83 then
		if(mbt)make_note((msy-3)/8,flr((msx-51)/8))
		if(mrt)del_note((msy-3)/8,flr((msx-51)/8))
		olp=(msy-3-moy)/8
	end
	if msx>84 and msx<84+33 then
		if(mbt)make_note(((msy-3)/8)+16,flr((msx-85)/8))
		if(mrt)del_note(((msy-3)/8)+16,flr((msx-85)/8))
		olp=((msy-3-moy)/8)+16
	end
	if(btnp(🅾️,0))copy_notes()
	if(btnp(⬅️,0))free=not free
	if(btnp(➡️,0))paste_notes()
	if btnp(❎,1) then
		ugh=not ugh
	end
	notelength+=stat(36)
	notelength=mid(0,notelength,64)
end

function _draw()
	cls()
	for x=4,7 do
		for y=0,15 do
			rectfill(x*8-1+20,y*8,x*8+6+20,y*8+7,6+(x+y)%2)
		end
	end
	for x=8,11 do
		for y=0,15 do
			rectfill(x*8+1+20,y*8,x*8+8+20,y*8+8,6+(x+y)%2)
		end
	end
	local moy=0
	if(not free)moy=(msy%8)-3
	if msx>50 and msx<83 then
		if msx<51+8 then
			spr(5,51,msy-3-moy)
		elseif msx<51+16 then
			spr(6,51+8,msy-3-moy)
		elseif msx<51+24 then
			spr(7,51+16,msy-3-moy)
		else
			spr(8,51+24,msy-3-moy)
		end
	elseif msx>84 and msx<84+33 then
		if msx<84+9 then
			spr(5,85,msy-3-moy)
		elseif msx<84+17 then
			spr(6,85+8,msy-3-moy)
		elseif msx<84+25 then
			spr(7,85+16,msy-3-moy)
		else
			spr(8,85+24,msy-3-moy)
		end
	end
	
	
	for _a in all(arrows) do
		if _a.t!=0 then
			line(54+(_a.d*8),_a.o*8+3,54+(_a.d*8),_a.o*8+(_a.t*8)+3,1)
		end
		if _a.ugh==1 then
			spr(255,51+(_a.d*8),_a.o*8)
		else
			spr(1+_a.d,51+(_a.d*8),_a.o*8)
		end
	end
	for _a in all(arrows) do
		if _a.t!=0 then
			line(88+(_a.d*8),(_a.o*8)-125,88+(_a.d*8),(_a.o*8)-125+(_a.t*8),1)
		end
		if _a.ugh==1 then
			spr(255,85+(_a.d*8),(_a.o*8)-128)
		else
			spr(1+_a.d,85+(_a.d*8),(_a.o*8)-128)
		end
	end
	spr(0,msx,msy)
	print("freeform\nnotes ⬅️\n"..(free and "on" or "off"),0,0,7)
	print("🅾️ to copy",0,38,7)
	
	print("note length:\n\n"..(ugh and "ugh" or "normal"),0,62,7)
	print("\n\n\ntoggle ugh(q)",0,0,7)
	print("⬆️ "..notelength.." ⬇️",0,68,7)
	print("(use\nscrollwheel)",0,80,7)
	print("ctrl+v then\n➡️ to paste",0,98,7)
	if(not free and olp!="") olp=flr(olp+0.45)
	print("\n\n\nbeat:"..olp,0,98,7)
end
-->8
--custom functions
function make_note(off,dir)
	if(not free)off=flr(off+0.4)
	local noarrow=true
	for _a in all(arrows) do
		if(_a.o==off and _a.d==dir)noarrow=false
	end
	local _a={
		o=off,
		d=dir,
		t=notelength,
		typ=0,
		ugh=ugh and 1 or 0
	}
	if(noarrow)add(arrows,_a)
end

function del_note(off,dir)
	if(not free)off=flr(off+0.4)
	for _a in all(arrows) do
		if(_a.o==off and _a.d==dir)del(arrows,_a)
	end
end

function paste_notes()
	if stat(4)!=nil and stat(4)!="" then
	arrows = {}
	local _ar = split(stat(4),":")
	for _o in all(_ar) do
		local _beat = split(_o)
		if _beat[3]==nil then
			_beat[3]=0
		end
		if _beat[4]==nil then
			_beat[4]=0
		end
		local _a={
			o=_beat[1],
			d=_beat[2]%4,
			t=_beat[3],
			ugh=_beat[4] or 0,
			typ=_beat[2],
		}
		_a.typ=flr(_a.typ/4)
		local noarrow=true
		for _a in all(arrows) do
			if(_a.o==_beat[1] and _a.d==_beat[2])noarrow=false
		end
		if(noarrow)add(arrows,_a)
	end
	end
end

function copy_notes()
	sort=arrows
	for e=0,#sort do
		for i=1,#sort-1 do
			if (sort[i].o > sort[i+1].o) then
				temp   = sort[i]
				sort[i]   = sort[i+1]
				sort[i+1] = temp
			end
		end
	end
	local str=""
	for _a in all(sort) do
		if(str!="")str=str..":"
		str=str.._a.o..",".._a.d
		if(_a.t>0  or _a.ugh!=0)str=str..","..(_a.t or 0)
		if _a.ugh!=0 then
			str=str..","..(_a.ugh or 0)
		end
	end
	printh(str,"@clip")
end
