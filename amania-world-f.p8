--background art

drumsize = 0
basssize = 0
arpsize = 0
melsize = 0
wpoints = {}
for i=0,8 do
	wpoints[i+1] = {x=i*16,y=63}
end
mpoints = {}
for i=0,8 do
	mpoints[i+1] = {x=i*16,y=63}
end

function background_draw()
	for i=0,3 do
		if stat(46+i)!=-1 then
			--address of current note
			local a=0x3200+68*stat(46+i)+(stat(50+i)<<1)
			--pitch of current note
			local p=%a&0x003f
			--volume of note
			local v=%a&0x0e00
			--waveform of note
			local w=%a&0x01c0
			
			if(i==0 and v>0) drumsize = v
			if(i==1) basssize = (p+4)*v/2000
			if(i==2) arpsize = p*8
			if(i==3) melsize = (p+4)*v/2000
			
		end
	end
	
	if arpsize > 0 then
		line(arpsize,0,0,arpsize,12)
		line(127-arpsize,0,127,arpsize,12)
		line(0,127-arpsize,arpsize,127,12)
		line(127-arpsize,127,127,127-arpsize,12)
	end
	
	for i=1,#wpoints-1 do
		local _r = basssize
		if(i>1) wpoints[i].y=63+(sin(time()*4+i/4)*_r)
		line(wpoints[i].x,wpoints[i].y,wpoints[i+1].x,wpoints[i+1].y,8)
	end
	
	if melsize > 0 then
		for i=1,#mpoints-1 do
			local _r = melsize
			if(i>1) mpoints[i].y=63+(cos(time()*4+i/4)*_r)
			line(mpoints[i].x,mpoints[i].y,mpoints[i+1].x,mpoints[i+1].y,11)
		end
		melsize -= 1
		if(melsize<0)melsize = 0
	end
	
	if drumsize > 0 then
		for j=0,drumsize/500 do
			rect(j,j,127-j,127-j,5)
		end
		drumsize -= 200
		if(drumsize < 0) drumsize = 0
	end
	
	if basssize > 0 then
		basssize -= 1
		if(basssize<0) basssize=0
	end
	
	--modcharting
	local _pat = stat(54)
	if _pat < 1 then
		for i=1,4 do
			roffsets[i].y = cos(step/(synctime/16)+i/2)*1
		end
	elseif _pat < 3 then
		for i=1,4 do
			roffsets[i].y = cos(step/(synctime/16)+i/2)*3
		end
	elseif _pat < 9 then
		for i=1,4 do
			roffsets[i].x = lerp(roffsets[i].x,sin(step/(synctime/4))*32,0.5)
			roffsets[i].y = 10+cos(step/(synctime/8))*10+cos(step/(synctime/16)+i/4)*6
		end
	elseif _pat < 11 then
		for i=1,4 do
			roffsets[i].x = lerp(roffsets[i].x,0,0.0325)
			roffsets[i].y = cos(step/(synctime/16)+i/2)*3
		end
	else
		for i=1,4 do
			roffsets[i].x = lerp(roffsets[i].x,sin(step/(synctime/4))*32,0.5)
			roffsets[i].y = 10+cos(step/(synctime/8))*10+cos(step/(synctime/16)+i/4)*6
		end
	end
	
end
