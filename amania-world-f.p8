	line(2,18,2,82,6)
	line(2,82,68,82,6)
	if graphreveal > 1 then
		for i=1,graphreveal-1 do
			local _ww = 68/(#healthgraph-1)
			local _gx1 = 2 + (i-1)*_ww
			local _gy1 = 82 - (healthgraph[i]/maxhp)*64
			local _gx2 = 2 + i*_ww
			local _gy2 = 82 - (healthgraph[i+1]/maxhp)*64
			line(_gx1,_gy1,_gx2,_gy2,8)
		end
